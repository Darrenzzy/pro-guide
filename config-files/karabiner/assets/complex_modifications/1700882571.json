{
  "title": "Zweih's Arrow Keys",
  "rules": [
    {
      "description": "Zweih's Arrow Keys (fn + ijkl to arrow keys)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "left_shift",
                "right_shift",
                "left_gui",
                "right_gui",
                "left_alt",
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "left_shift",
                "right_shift",
                "left_gui",
                "right_gui",
                "left_alt",
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "left_shift",
                "right_shift",
                "left_gui",
                "right_gui",
                "left_alt",
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "left_shift",
                "right_shift",
                "left_gui",
                "right_gui",
                "left_alt",
                "right_alt"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            },
            "key_code": "q"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "escape"
            }
          ]
        }
      ]
    }
  ]
}
